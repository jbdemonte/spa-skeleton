body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

#main
    border 1px solid #c0c0c0
    width 250px
    text-align center
    padding 20px
    margin 0 auto

    button
        border 1px solid #A7A7A7
        background-color #fff
        cursor pointer
        padding 2px 5px
        margin-top 5px

        &:hover
            background-color #BDBDBD
            color #fff